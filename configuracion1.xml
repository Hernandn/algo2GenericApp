<?xml version="1.0" encoding="UTF-8"?>
<application name="git">
    <application name="commit">
        <path>C:\git\git.exe</path>
        <flag>commit<flag/>
        <parameter>
            <flag>-m</flag>
            <id>textMessage</id>
        </parameter>
        <window>
            <label>Mensaje</label>
            <textBox id="textMessage"></textBox>
        </window>
        <flag>-v</flag>
        <validation>
            <textMessage maxSize=30>
        </validation>
    </application>    
</application>
